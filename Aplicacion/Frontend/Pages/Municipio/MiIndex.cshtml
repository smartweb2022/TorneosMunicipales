@page
@model Frontend.Pages.MiIndexModel
@{
    ViewData["Title"]= "MiIndex";
}
<h1>Lista de Municipio</h1>
<p> 
    <a asp-page= "Create"> Crear Municipio</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th> Id </th>   
            <th> Nombre </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var mun in Model.Municipios)
        {
        <tr>
            <td> @mun.Id </td>
            <td> @mun.Nombre </td>
            <td>
                <a asp-page="./EditMuni" asp-route-id="@mun.Id">Editar</a>
                <a asp-page="./DeleteMuni"asp-route-id="@mun.Id">Eliminar</a>
                <a asp-page="./DetailsMuni"asp-route-id="@mun.Id">Detalles</a>
            </td>
        </tr>
        }
    </tbody>
</table>